#include<stdio.h>
int main(){
	int a[100],i,j,n,a;
	printf("enter the size of array");
	scanf("%d",&n);
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
		}
	for(i=0;i<n;i++){
		for(j=i+1;i<n;i++){
			if(a[i]>a[j]){
				a=a[i];
				a[i]=a[j];
				a[j]=;
			}
		}
	}
	printf("numbers after sorting");
	for(i=0;i<n;i++){
		printf("%d  ",a[i]);
	}
}
